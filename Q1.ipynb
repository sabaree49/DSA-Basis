   "source": [
    "def sum_pairs(arr, n, sum):\n",
    "    count = 0\n",
    "    for i in range(0, n):\n",
    "        for j in range(i + 1, n):\n",
    "            if arr[i] + arr[j] == sum:\n",
    "                count += 1\n",
    "    return count\n",
    " \n",
    "arr = [1,2,3,5,6,8,2,4]\n",
    "n = len(arr)\n",
    "sum = 5\n",
    "print(f\"Count of pairs is, {sum_pairs(arr, n, sum)}\")"
   ]
